<?xml version="1.0" encoding="utf-8"?>
<plugin xmlns="http://www.phonegap.com/ns/plugins/1.0"
        id="kaweb-cordova-plugin-filespace"
        version="0.0.3">

  <name>KawebCordovaPluginFileSpace</name>

  <engines>
    <engine name="cordova" version=">=3.4.0"/>
  </engines>
        
  <platform name="ios">
    
    <config-file target="config.xml" parent="/widget">
      <feature name="KawebCordovaPluginFileSpace">
        <param name="ios-package" value="KawebCordovaPluginFileSpace" />
      </feature>
    </config-file>

    <header-file src="src/ios/KawebCordovaPluginFileSpace.h" target-dir="FileSpacePlugin"/>
    <source-file src="src/ios/KawebCordovaPluginFileSpace.m" target-dir="FileSpacePlugin"/>
  </platform>

</plugin>
